<Visualization::PageSetup::Sidebar::Toolbar::TraceReplayer::TraceSelection
  @moveCameraTo={{@moveCameraTo}}
  @selectTrace={{this.selectTrace}}
  @dynamicData={{@dynamicData}}
  @structureData={{@structureData}}
  @application={{@application}}
  @applicationTraces={{this.applicationTraces}}
  @selectedTrace={{this.selectedTrace}}
  @unit={{this.unit}}
  @toggleUnit={{this.toggleUnit}}
/>

{{#if this.selectedTrace}}
  <Visualization::PageSetup::Sidebar::Toolbar::TraceReplayer::TraceReplayerMain
    @selectedTrace={{this.selectedTrace}}
    @structureData={{@structureData}}
    @dynamicData={{@dynamicData}}
    @renderingLoop={{@renderingLoop}}
    @highlightTrace={{@highlightTrace}}
    @landscapeData={{@landscapeData}}
    @unit={{this.unit}}
  />
{{/if}}