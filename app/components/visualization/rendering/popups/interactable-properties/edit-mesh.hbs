<hr> 
{{#if this.isEntityPackage}}
    <BsButton @type='success' {{on 'click' this.addPackage}}>
        {{svg-jar this.iconName class='octicon align-right'}}
        <BsTooltip @placement='top' @triggerEvents='hover'>
            Add Package
        </BsTooltip>
    </BsButton>
    <BsButton @type='info' {{on 'click' this.addClass}}>
        {{svg-jar this.iconName class='octicon align-right'}}
        <BsTooltip @placement='top' @triggerEvents='hover'>
            Add Class
        </BsTooltip>
    </BsButton>
    <BsButton @type='danger' {{on 'click' this.deleteMesh}}>
        {{svg-jar this.iconName class='octicon align-right'}}
        <BsTooltip @placement='top' @triggerEvents='hover'>
            Delete
        </BsTooltip>
    </BsButton>
    <BsButton @type='dark' {{on 'click' this.cutMesh}}>
        {{svg-jar this.iconName class='octicon align-right'}}
        <BsTooltip @placement='top' @triggerEvents='hover'>
            Cut
        </BsTooltip>
    </BsButton>
    <BsButton @type='light' {{on 'click' this.insertMesh}}>
        {{svg-jar this.iconName class='octicon align-right'}}
        <BsTooltip @placement='top' @triggerEvents='hover'>
            Insert
        </BsTooltip>
    </BsButton>
{{else if this.isEntityApplication}}
    <BsButton @type='success' {{on 'click' this.addPackage}}>
        {{svg-jar this.iconName class='octicon align-right'}}
        <BsTooltip @placement='top' @triggerEvents='hover'>
            Add Package
        </BsTooltip>
    </BsButton>
    <BsButton @type='danger' {{on 'click' this.deleteMesh}}>
        {{svg-jar this.iconName class='octicon align-right'}}
        <BsTooltip @placement='top' @triggerEvents='hover'>
            Delete
        </BsTooltip>
    </BsButton>
{{else if this.isEntityClass}}
    <BsButton @type='danger' {{on 'click' this.deleteMesh}}>
        {{svg-jar this.iconName class='octicon align-right'}}
        <BsTooltip @placement='top' @triggerEvents='hover'>
            Delete
        </BsTooltip>
    </BsButton>
    <BsButton @type='danger' {{on 'click' this.cutMesh}}>
        {{svg-jar this.iconName class='octicon align-right'}}
        <BsTooltip @placement='top' @triggerEvents='hover'>
            Cut
        </BsTooltip>
    </BsButton>
    <BsButton @type='danger' {{on 'click' this.insertMesh}}>
        {{svg-jar this.iconName class='octicon align-right'}}
        <BsTooltip @placement='top' @triggerEvents='hover'>
            Insert
        </BsTooltip>
    </BsButton>
{{/if}}
